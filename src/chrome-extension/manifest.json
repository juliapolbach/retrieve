{
        "manifest_version": 3,
        "name": "Question Generator",
        "version": "1.0",
        "description": "Generate questions from selected text on any webpage",
        "permissions": [
          "activeTab",
          "storage"
        ],
        "action": {
          "default_popup": "popup.html"
        },
        "content_scripts": [
          {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
          }
        ]
      }
      